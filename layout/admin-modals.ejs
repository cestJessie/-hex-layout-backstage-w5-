<!-- view Admin -->
<div class="modal fade view-admin" id="viewAdmin" tabindex="-1" aria-labelledby="viewAdminLabel" aria-hidden="true">
  <div class="modal-dialog">
    <div class="modal-content">
      <div class="modal-body d-flex p-7">
        <div class="me-6">
          <img src="../assets/images/emir.jpeg" alt="Emir-photo" class="Emir-photo rounded-1">
        </div>
        <div class="w-100 px-0">
          <div class="d-flex justify-content-between">
            <p class="mb-0">#1</p>
            <button type="button" class="btn-close rounded-0" data-bs-dismiss="modal" aria-label="Close">
            </button>
          </div>
          <p class="mb-0">Admin</p>
          <h4 class="fw-bold mb-3">Emir Wicks</h4>
          <p class="mb-5 fs-7 text-secondary">emir.wicks@mail.com</p>
          <div class="accordion accordion-flush" id="accordionFlushExample">
            <div class="accordion-item">
              <h2 class="accordion-header d-flex justify-content-between" id="flush-headingOne">
                <p class="text-center p-3 bg-light fs-7">Verified</p>
                <button class="accordion-button collapsed px-0" type="button" data-bs-toggle="collapse"
                  data-bs-target="#flush-collapseOne" aria-expanded="false" aria-controls="flush-collapseOne">
                </button>
              </h2>
              <div id="flush-collapseOne" class="accordion-collapse collapse" aria-labelledby="flush-headingOne"
                data-bs-parent="#accordionFlushExample">
                <div class="accordion-body p-0">
                  <div class="d-flex justify-content-between p-0">
                    <button type="button"
                      class="btn btn-lg btn-outline-danger px-7 py-3 rounded-1 fs-6">Deactivate</button>
                    <button type="button" class="btn btn-lg btn-success px-7 py-3 rounded-1 fs-6">Edit</button>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>

<!-- edit admin -->
<div class="modal fade edit-admin " id="editAdmin" tabindex="-1" aria-labelledby="editAdminLabel" aria-hidden="true">
  <div class="modal-dialog modal-dialog-centered">
    <div class="modal-content">
      <div class="modal-header p-7 border-bottom-0">
        <h4 class="modal-title fw-bold" id="editAdminLabel">Edit Admin</h4>
        <button type="button" class="btn-close rounded-0" data-bs-dismiss="modal" aria-label="Close"></button>
      </div>
      <div class="modal-body px-7 py-0 mb-0">
        <div class="edit-account-area border-bottom pb-6 mb-6">
          <h5 class="mb-5 fw-bold">Account</h5>
          <div class="edit-account-name mb-5">
            <label for="edit-account-name" class="form-label fs-7">Name</label>
            <input type="text" name="edit-account-name" id="edit-account-name"
              class="form-control p-5 bg-light border-0 rounded-1" value="Emir Wicks">
          </div>
          <div class="edit-account-mail">
            <label for="edit-account-mail" class="form-label fs-7">Email</label>
            <input type="text" name="edit-account-mail" id="edit-account-mail"
              class="form-control p-5 bg-light border-0 rounded-1" value="emir.wicks@mail.com">
          </div>
        </div>
        <div class="edit-verification-area border-bottom pb-6 mb-6">
          <h5 class="mb-5 fw-bold">Verification</h5>
          <div class="d-flex justify-content-between mb-5">
            <div class="d-flex me-2">
              <span class="material-icons me-3 text-success mb-0">check_circle</span>
              <p class="fw-bold mb-0">Verified</p>
            </div>
            <div>
              <date class="fs-7 mb-0">2020/4/12</date>
            </div>
          </div>
          <div>
            <button class="btn border rounded-0 w-100 py-5 px-0 text-black-50" disabled>Send Verification Mail</button>
          </div>
        </div>
        <div class="edit-access-level-area border-bottom pb-6 mb-6">
          <h5 class="mb-5 fw-bold">Access Level</h5>
          <div class="dropdown">
            <button
              class="btn dropdown-toggle w-100 d-flex justify-content-between align-items-center p-5 rounded-1 btn-outline-secondary"
              type="button" id="edit-access-dropdown-Button" data-bs-toggle="dropdown" aria-expanded="false">
              Admin
            </button>
            <ul class="dropdown-menu" aria-labelledby="edit-access-dropdown-Button">
              <li><a class="dropdown-item" href="#">Access Level #1</a></li>
              <li><a class="dropdown-item" href="#">Access Level #2</a></li>
              <li><a class="dropdown-item" href="#">Access Level #3</a></li>
            </ul>
          </div>
        </div>

        <div class="edit-custom-access-area border-bottom pb-6 mb-6">
          <h5 class="mb-5 fw-bold">Custom Access</h5>
          <div class="form-check mb-5">
            <input class="form-check-input me-4" type="checkbox" value="" id="coupon-check">
            <label class="list-group-item">Lecture</label>
          </div>
          <div class="form-check mb-5">
            <input class="form-check-input me-4" type="checkbox" value="" id="coupon-check">
            <label class="list-group-item">Coupon</label>
          </div>
          <div class="form-check mb-5">
            <input class="form-check-input me-4" type="checkbox" value="" id="message-check">
            <label class="list-group-item">Message</label>
          </div>
          <div class="form-check mb-5">
            <input class="form-check-input me-4" type="checkbox" value="" id="setting-check">
            <label class="list-group-item">Setting</label>
          </div>
          <div class="form-check">
            <input class="form-check-input me-4" type="checkbox" value="" id="task-menu-check">
            <label class="list-group-item">Task Menu</label>
          </div>
        </div>
        <div class="edit-deactivate-account-area pb-6 mb-6">
          <h5 class="mb-5 fw-bold">Deactivate Account</h5>
          <div class="d-flex text-danger mb-5">
            <span class="material-icons me-3 mb-0">warning</span>
            <p class="mb-0">This cannot be undone!</p>
          </div>
          <button class="btn btn-outline-danger w-100 py-5 px-0 fw-bold rounded-1">Deactivate This Account</button>
        </div>
      </div>
      <div class="modal-footer border-top-0 pe-6">
        <button type="button" class="btn btn-outline-success py-3 px-7 rounded-1 me-5"
          data-bs-dismiss="modal">Cancle</button>
        <button type="button" class="btn btn-success py-3 px-7 rounded-1">Update</button>
      </div>
    </div>
  </div>
</div>

